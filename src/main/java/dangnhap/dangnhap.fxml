<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--Write a sign in user interface -->

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dangnhap.DangnhapUiController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Đăng nhập" />
        <Label layoutX="14.0" layoutY="60.0" text="Tên đăng nhập" />
        <TextField id="usernameTextField" fx:id="usernameTextField" layoutX="14.0" layoutY="80.0" />
        <Label layoutX="14.0" layoutY="120.0" text="Mật khẩu" />
        <PasswordField id="passwordTextField" fx:id="passwordTextField" layoutX="14.0" layoutY="140.0" />
        <Button fx:id="btnLogin" layoutX="14.0" layoutY="180.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Đăng nhập" />
        <Button fx:id="btnRegister" layoutX="14.0" layoutY="220.0" mnemonicParsing="false" onAction="#registerButtonClicked" text="Đăng ký" />
    </children>

</AnchorPane>
